{
    "name": "first-network-${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "${ORGMSP}",
        "tlsEnable": true,
		"adminUser": "admin",
		"adminPassword": "adminpw",
		"enableAuthentication": false,
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORGMSP}": {
            "mspid": "${ORGMSP}MSP",
            "peers": [
                "peer0.${ORG}.healthcare.com",
                "peer1.${ORG}.healthcare.com"
            ],
            "certificateAuthorities": [
                "ca.${ORG}.healthcare.com"
            ]
        }
    },
    "peers": {
        "peer0.${ORG}.healthcare.com": {
            "url": "grpcs://peer0.${ORG}.healthcare.com:${P0PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.${ORG}.healthcare.com",
                "hostnameOverride": "peer0.${ORG}.healthcare.com"
            }
        },
        "peer1.${ORG}.healthcare.com": {
            "url": "grpcs://peer1.${ORG}.healthcare.com:${P1PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.${ORG}.healthcare.com",
                "hostnameOverride": "peer1.${ORG}.healthcare.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.${ORG}.healthcare.com": {
            "url": "https://ca.${ORG}.healthcare.com:${CAPORT}",
            "caName": "ca.${ORG}.healthcare.com",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
